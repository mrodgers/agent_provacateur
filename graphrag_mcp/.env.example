# GraphRAG MCP Server Configuration

# Server port
PORT=8083

# Vector database URL
VECTOR_DB_URL=http://localhost:6333

# Cache configuration
ENABLE_CACHE=true
MAX_CACHE_SIZE=1000
CACHE_TTL=3600

# Query configuration
MAX_RESULTS=10
MIN_CONFIDENCE=0.5
TRAVERSAL_DEPTH=2

# Rate limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info